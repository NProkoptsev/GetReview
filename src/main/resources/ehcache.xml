<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="com.getreviews.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Review"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Item"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Client"
        timeToLiveSeconds="3600">
    </cache>


    <cache name="com.getreviews.domain.Source"
        timeToLiveSeconds="3600">
    </cache>


    <cache name="com.getreviews.domain.Image"
        timeToLiveSeconds="3600">
    </cache>


    <cache name="com.getreviews.domain.Client.reviews"
           timeToLiveSeconds="3600">
    </cache>


    <cache name="com.getreviews.domain.Category"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Sale"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Sale.reviews"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Sale.items"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Item.images"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Item.reviews"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Item.sales"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.getreviews.domain.Bookmark"
        timeToLiveSeconds="3600">
    </cache>

    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
